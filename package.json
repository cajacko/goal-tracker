{
  "name": "goal-tracker",
  "version": "0.1.0",
  "main": "index.js",
  "repository": "https://github.com/cajacko/goal-tracker.git",
  "author": "Charlie Jackson <contact@charliejackson.com>",
  "license": "MIT",
  "scripts": {
    "login": "firebase login",
    "init": "firebase init functions",
    "deploy": "yarn run build && firebase deploy --only functions",
    "start": "npm-run-all -p watch:css start:js",
    "start:js": "react-scripts start",
    "build:js": "react-scripts build",
    "build": "npm-run-all build:css build:js",
    "build:css": "node-sass-chokidar src/ -o src/",
    "watch:css": "npm run build:css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "eject": "react-scripts eject",
    "serve": "yarn run build && firebase serve",
    "precommit": "lint-staged",
    "prettify": "prettier-eslint --write 'src/**/*.{js,jsx}'"
  },
  "lint-staged": {
    "*.js": [
      "prettier-eslint --write",
      "git add"
    ],
    "*.jsx": [
      "prettier-eslint --write",
      "git add"
    ],
    "*.md": [
      "prettier-eslint --write",
      "git add"
    ]
  },
  "dependencies": {
    "firebase-tools": "^3.17.4"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "eslint": "^4.18.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-babel-module": "^4.0.0",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jest": "^21.12.2",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "firebase-tools": "^3.17.4",
    "flow-bin": "^0.66.0",
    "fs-extra": "^5.0.0",
    "husky": "^0.14.3",
    "jsdoc": "^3.5.5",
    "jsdoc-babel": "^0.3.0",
    "lint-staged": "^7.0.0",
    "node-sass-chokidar": "^0.0.3",
    "npm-run-all": "^4.1.2",
    "prettier-eslint-cli": "^4.7.1",
    "prop-types": "^15.6.0",
    "raven-js": "^3.23.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-scripts": "1.1.0",
    "react-slick": "^0.18.1",
    "slick-carousel": "^1.8.1"
  }
}
